/* pages/list/list.ttss */
.img-item {
  width: calc(33.3% - 7px);
  height:calc(33VW - 7px);
  margin-bottom: 10px;
  margin-right: 10px;
  box-sizing: border-box;
  border-radius: 5px;
}

.long-img-item {
  width: calc(33.3% - 7px);
  height: calc(66VW - 7px);
  margin-bottom: 10px;
  margin-right: 10px;
  box-sizing: border-box;
  border-radius: 5px;
}

.img-container {
  padding: 8px;
  box-sizing: border-box;
}

.lastIndex {
  margin-right: 0;
}