/* pages/list/list.ttss */
.img-item {
  width: 33.3%;
  height: 5rem;
  padding-bottom: 5px;
  padding-right: 5px;
  box-sizing: border-box;
  border-radius: 5px;
}

.long-img-item {
  width: 33.3%;
  height: 9rem;
  padding-bottom: 5px;
  padding-right: 5px;
  box-sizing: border-box;
  border-radius: 5px;
}

.img-container {
  padding: 8px;
  box-sizing: border-box;
}

.lastIndex {
  padding-right: 0;
}